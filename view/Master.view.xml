<core:View
	controllerName="sap.ui.demo.myFiori2.view.Master"
	xmlns="sap.m"
	xmlns:core="sap.ui.core" >
	<Page
		title="{i18n>MasterTitle}" >
		<List
			items="{/StylesCollection}" >
			<ObjectListItem
				type="Active"
				press="handleListItemPress"
				Id="{Id}"
				Name="{Name}"
				Customer="{Customer}"
				Department="{Department}" >
				<attributes>
					<ObjectAttribute text="{Name}" />
					<ObjectAttribute text="{Customer}" />
					<ObjectAttribute text="{Department}" />
				</attributes>
				<firstStatus>
					<ObjectStatus text="{Status}" />
				</firstStatus>
			</ObjectListItem>
		</List>
	</Page>
</core:View>